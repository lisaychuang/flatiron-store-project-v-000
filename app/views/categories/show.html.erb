<h1><%= @category.title %></h1>

<ul>
    <% @category.items.each do |item| %>
        <li><%= link_to "#{item.title}, $#{item.price/100.0}", item_path(item) %></li>
    <% end %>

     <%#if @current_user %>
        <%= form_tag carts_path do %>
        <%= submit_tag "Add to Cart" %>
        <% end %>
    <%# end %>
</ul>